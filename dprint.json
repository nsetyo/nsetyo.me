{
  "exec": {
    "associations": ["**/*.astro", "**/*.ts", "**/*.tsx", "!**/*.astro#.{ts,tsx}"],
    "commands": [
      {
        "command": "biome check --fix --unsafe --stdin-file-path {{file_path}}",
        "exts": ["astro", "ts", "tsx"]
      }
    ]
  },
  "json": {},
  "malva": {},
  "markdown": {
    "textWrap": "always"
  },
  "markup": {
    "associations": [
      "**/*.astro",
      "**/*.html"
    ],
    "printWidth": 100,
    "component.selfClosing": true,
    "maxAttrsPerLine": 1,
    "useTabs": true
  },
  "toml": {},
  "typescript": {
    "associations": ["**/*.{js,ts}", "**/*.{jsx,tsx}"],
    "module.sortImportDeclarations": "maintain",
    "lineWidth": 100,
    "quoteProps": "consistent",
    "quoteStyle": "preferSingle",
    "semiColons": "asi",
    "useTabs": true
  },
  "yaml": {},
  "excludes": ["**/node_modules"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.0.wasm",
    "https://plugins.dprint.dev/markdown-0.18.0.wasm",
    "https://plugins.dprint.dev/json-0.20.0.wasm",
    "https://plugins.dprint.dev/toml-0.7.0.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.11.2.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.19.1.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.0.wasm",
    "https://plugins.dprint.dev/exec-0.5.1.json@492414e39dea4dccc07b4af796d2f4efdb89e84bae2bd4e1e924c0cc050855bf"
  ]
}
