{
  "editor.defaultFormatter": "dprint.dprint",
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 100,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.wordWrap": "off"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint"
  }
}
